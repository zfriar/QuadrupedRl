<?xml version="1.0" ?>
<robot name="ant">
  <!-- Minimal ant URDF: base_link plus 8 simple revolute joints each with a child link.
       This file provides joint indices 0..7 so the test environment can query joint states.
       Visuals and collisions are intentionally minimal. -->
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.1"/>
    </inertial>
  </link>

  <!-- Create 8 child links and revolute joints attached to the base_link -->
  <!-- Each joint has limits and an axis so PyBullet reports joint state and allows motor control -->
  
  <link name="link0">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.01" iyy="0.01" izz="0.01"/>
    </inertial>
  </link>
  <joint name="joint0" type="revolute">
    <parent link="base_link"/>
    <child link="link0"/>
    <origin xyz="0.1 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14159" upper="3.14159" effort="10" velocity="10"/>
  </joint>

  <link name="link1">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.01" iyy="0.01" izz="0.01"/>
    </inertial>
  </link>
  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="-0.1 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14159" upper="3.14159" effort="10" velocity="10"/>
  </joint>

